{
  "name": "Electronjs-MohsinNotification",
  "version": "1.0.0",
  "description": "Making and testing electron notification",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
"package-win": "electron-packager . MohsinNotificationS -overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icons.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"mohsinNotification\"",    
"package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/icon.png --prune=true --out=release-builds"
  
  },
  "keywords": [],
  "author": "MohsinDev369",
  "license": "MIT",
  "devDependencies": {
    "electron": "^14.0.0",
    "electron-packager": "^15.3.0"
  }
}
